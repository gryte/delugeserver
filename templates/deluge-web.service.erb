[Unit]
Description=Deluge Bittorrent Client Web Interface
After=network-online.target

[Service]
Type=simple

User=deluge
Group=deluge
UMask=027

ExecStart=/usr/bin/deluge-web <%= node['deluge']['daemon']['logs']['file'] %> <%= node['deluge']['daemon']['logs']['level'] %>

Restart=on-failure

[Install]
WantedBy=multi-user.target
